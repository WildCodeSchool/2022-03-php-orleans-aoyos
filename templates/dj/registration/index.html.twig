{% extends 'base.html.twig' %}

{% block title %}
	{{ parent() }}
	- S'inscrire
{% endblock %}

{% block stylesheets %}
	{{ parent() }}
	{{ encore_entry_link_tags('registration') }}
{% endblock %}

{% block javascripts %}
	{{ parent() }}
{% endblock %}

{% block body %}
	<main class="container py-5">
		<h1 class="text-center mb-5">Inscription</h1>
		{{ form_start(form) }}
		{{ form_widget(form) }}
		<div class="d-flex justify-content-between">
			<a href="{{ path('app_admin_partner_index') }}" class="btn btn-outline-primary pb-0 px-3">
				<i class="bi bi-arrow-left-square"></i>
				Retour
			</a>
			<button class="btn btn-outline-primary pb-0 px-3">
				<i class="bi bi-plus"></i>
				S'inscrire
			</button>
		</div>
		{{ form_end(form) }}
	</main>
{% endblock %}
